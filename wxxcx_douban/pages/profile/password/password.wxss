.login_page{
	display: flex;
	flex-direction: column;
}
.login_from{
	display: flex;
	flex-direction: column;
	height:200rpx;
	width:90%;
	margin:100rpx auto 50rpx;
	border:2rpx solid #999;
	border-radius:8rpx;
	background-color: #fff;
}
.lo_input{
	display: flex;
	height: 49%;
	padding-left:20rpx;
	text-align: left;
	font-size: 34rpx;
	color: #666;
}
.lo_input:nth-of-type(1){
	border-bottom: 2rpx solid #999;
}
.denglu{
	height: 80rpx;
	width: 90%;
	margin: 0 auto;
	background-color: #48C23D;
	text-align: center;
	line-height: 80rpx;
	border-radius: 8rpx;
	font-size: 40rpx;
	font-family: "Microsoft YaHei";
	color: #fff;
	letter-spacing: 4rpx;
	font-weight: bold;
}
.yanzheng{
	color: #48C23D;
	font-size:22rpx;
	font-family: "Microsoft YaHei";
	letter-spacing: 2rpx;
	line-height: 100rpx;
	padding-left: 20rpx;
	border-left: 2rpx solid #999;
	position: relative;
	left:90rpx;
	top: 0;
}